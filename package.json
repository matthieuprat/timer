{
  "name": "timer",
  "description": "A timer for the minimalist",
  "version": "0.0.0",
  "repository": "https://github.com/matthieuprat/timer",
  "scripts": {
    "watch": "npm run build -- --watch",
    "lint": "$(npm bin)/standard timer.js",
    "build": "babel timer.js --out-file timer.dist.js"
  },
  "babel": {
    "presets": [ "es2015" ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.0",
    "standard": "^5.4.1"
  },
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*.dist.js"
    ]
  }
}
